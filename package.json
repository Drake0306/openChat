{
  "name": "openchat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:setup": "tsx scripts/dev-setup.ts && npm run dev",
    "dev:setup:win": "dev-setup.bat",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && prisma db push && tsx prisma/seed.ts && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "prisma db push --force-reset && tsx prisma/seed.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:viewer": "tsx scripts/db-viewer.ts",
    "test:deployment": "tsx scripts/test-deployment.ts",
    "db:fix-windows": "tsx scripts/fix-prisma-windows.ts",
    "docker:dev": "node -e \"process.platform === 'win32' ? require('child_process').exec('scripts\\\\dev-setup.bat') : require('child_process').exec('scripts/dev-setup.sh')\"",
    "docker:build": "node -e \"process.platform === 'win32' ? require('child_process').exec('scripts\\\\prod-build.bat') : require('child_process').exec('scripts/prod-build.sh')\"",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod-stop": "docker-compose -f docker-compose.prod.yml down",
    "docker:reset": "node -e \"process.platform === 'win32' ? require('child_process').exec('scripts\\\\db-reset.bat') : require('child_process').exec('scripts/db-reset.sh')\"",
    "docker:cleanup": "node -e \"process.platform === 'win32' ? require('child_process').exec('scripts\\\\cleanup.bat') : require('child_process').exec('scripts/cleanup.sh')\"",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.7.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^2.2.31",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.309.0",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "openai": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "react-markdown": "^9.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "sonner": "^1.3.1",
    "stripe": "^14.12.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
